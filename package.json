    "html-webpack-plugin": "^1.1.0",
    "jest-cli": "^0.4.0",
    "jquery": "^2.1.1",
    "jsx-loader": "^0.12.0",
    "prismjs": "git+https://github.com/PrismJS/prism.git#master",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.2.5",
    "react-prism": "^1.3.0",
